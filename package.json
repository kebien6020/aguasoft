{
  "scripts": {
    "build-server-js": "copyfiles -f ./server/src/db/config.json ./server/dist/db && tsc -p server",
    "watch-server-js": "yarn build-server-js -w",
    "start-server": "node server/dist/index.js",
    "start-server-autoreload": "nodemon server/dist/index.js -w server/dist",
    "dev-server": "npm-run-all -p watch-server-js start-server-autoreload",
    "build-client-dev": "webpack --config client/webpack.dev.js",
    "build": "webpack --config client/webpack.prod.js",
    "watch-client-js": "yarn build-client-dev -w",
    "dev": "npm-run-all -p dev-server watch-client-js",
    "analyze": "webpack-bundle-analyzer ./client/stats.json",
    "migrate": "sequelize db:migrate",
    "migrate-prod": "sequelize db:migrate --env production"
  },
  "devDependencies": {
    "@date-io/moment": "^1.3.5",
    "@material-ui/core": "^3.9.3",
