    "@tsconfig/node22": "^22.0.0",
    "@types/auth0-js": "^9.14.7",
    "@types/bcryptjs": "^2.4.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/date-fns": "^2.6.3",
    "@types/debug": "^4.1.7",
    "@types/express-session": "^1.17.7",
    "@types/faker": "^5.5.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^22.13.4",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.12",
    "@types/umzug": "^2.3.3",
    "auth0-js": "^9.23.2",
