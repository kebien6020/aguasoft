{
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "babel src -d dist -x .ts,.js --delete-dir-on-start",
    "dev": "node --import tsx --watch --watch-path=src src/index.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "migrate": "npm run build && sequelize db:migrate",
    "start": "node dist/index.js",
    "start-dev": "node --import tsx src/index.ts",
    "test": "tsx test.ts",
    "test:junit": "tsx test.ts --test-reporter=junit --test-reporter-destination=test-report.xml",
    "test:update-snapshots": "tsx test.ts --test-update-snapshots",
    "test:only": "tsx test.ts --test-only",
    "watch-run": "node --watch dist/index.js",
    "watch-build": "npm run build -- -w"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "buffer": "^6.0.3",
    "connect-session-sequelize": "^8.0.2",
    "cors": "^2.8.4",
    "date-fns": "^4.1.0",
    "debug": "^4.4.3",
    "express-jwt": "^8.5.1",
    "express-session": "^1.18.2",
    "jwks-rsa": "^3.2.0",
    "qs": "^6.14.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "ultimate-express": "^2.0.12",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@faker-js/faker": "^10.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.7",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "glob": "^11.0.3",
    "npm-run-all": "^4.1.5",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
