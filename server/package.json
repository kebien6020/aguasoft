{
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "babel src -d dist -x .ts,.js --delete-dir-on-start",
    "dev": "node --import tsx --watch --watch-path=src src/index.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "migrate": "npm run build && sequelize db:migrate",
    "start": "node dist/index.js",
    "start-dev": "node --import tsx src/index.ts",
    "test": "tsx test.ts",
    "test:update-snapshots": "tsx test.ts --test-update-snapshots",
    "test:only": "tsx test.ts --test-only",
    "watch-run": "node --watch dist/index.js",
    "watch-build": "npm run build -- -w"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "buffer": "^6.0.3",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.4",
    "date-fns": "^4.1.0",
    "debug": "^4.4.0",
    "express": "^5.0.1",
    "express-jwt": "^8.5.1",
    "express-session": "^1.18.1",
    "jwks-rsa": "^3.1.0",
    "qs": "^6.14.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.20.0",
    "@faker-js/faker": "^9.5.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/bcryptjs": "^2.4.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express-session": "^1.17.7",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "@types/umzug": "^2.3.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "glob": "^11.0.1",
    "npm-run-all": "^4.1.5",
    "supertest": "^7.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
