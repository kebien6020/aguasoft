    "cors": "^2.8.4",
    "date-fns": "^4.1.0",
    "debug": "^4.4.0",
    "express": "^5.0.1",
    "express-jwt": "^6.1.2",
    "express-session": "^1.18.1",
    "jwks-rsa": "^1.12.3",
    "qs": "^6.14.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^2.5.0",
    "sqlite3": "^5.1.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
