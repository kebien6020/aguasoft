    "faker": "^5.5.3",
    "glob": "^11.0.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "umzug": "^2.3.0"
